    body {
      font-family: cralika !important;
      color: #245656 !important;
    }
    .custom-button {
      font-weight: bold;
      color: #245656;
      border: 1px solid #245656;
      border-radius: 30px;
      padding: 5px 15px;
      background-color: transparent;
    }
    .custom-button:hover {
      background-color: #245656;
      color: white;
    }
    .service-container {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      gap: 20px;
    }

    .service-card {
      position: relative;
      overflow: hidden;
      border-radius: 10px;
      width: 300px;
      height: 400px;
      flex: 1 1 300px;
    }

    .service-card img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .service-card .overlay {
      position: absolute;
      bottom: 0;
      left: 0;
      right: 0;
      background: rgba(0, 0, 0, 0.194);
      color: white;
      padding: 10px;
      text-align: start;
    }

    #smallsection {
      margin-top: 30px;
      width: 100%;
      height: 250px;
      background-color: #245656;
    }

    @media (max-width: 768px) {
      .service-card {
        width: 45%;
        flex: 1 1 45%;
      }

      #smallsection {
        padding: 18px;
      }
    }

    @media (max-width: 576px) {
      .service-card {
        width: 100%;
        flex: 1 1 100%;
      }

      #smallsection {
        padding: 18px;
      }
    }

    .btn-more-services {
      display: block;
      margin: 20px auto;
      text-align: center;
    }

    .custom-height {
      height: 500px; /* Set your desired height */
      object-fit: cover; /* To maintain aspect ratio and cover the area */
    }

  </style>

